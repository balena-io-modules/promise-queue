{
  "name": "@balena/promise-queue",
  "version": "1.2.4",
  "description": "Queue sync or async functions with max concurrency, max size, max age, and fifo/lifo ordering",
  "author": "Pagan Gazzard <page@balena.io>",
  "main": "out/index.js",
  "license": "Apache-2.0",
  "types": "out/index.d.ts",
  "scripts": {
    "prepublish": "require-npm4-to-publish",
    "prepublishOnly": "npm run lint",
    "prepare": "npm run build",
    "build": "npx tsc",
    "lint": "resin-lint --typescript src/ && tsc --noEmit",
    "test": "npm run lint",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"src/**/*.ts\""
  },
  "homepage": "https://github.com/balena-io-modules/promise-queue",
  "repository": {
    "type": "git",
    "url": "https://github.com/balena-io-modules/promise-queue.git"
  },
  "bugs": {
    "url": "https://github.com/balena-io-modules/promise-queue/issues"
  },
  "dependencies": {
    "eventemitter3": "^3.1.0",
    "typed-error": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^10.13.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "require-npm4-to-publish": "^1.0.0",
    "resin-lint": "^2.0.1",
    "typescript": "^3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --config ./node_modules/resin-lint/config/.prettierrc --write",
      "resin-lint --typescript",
      "git add"
    ]
  }
}
